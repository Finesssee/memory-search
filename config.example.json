{
  "sources": [
    "/path/to/your/notes",
    "/path/to/more/docs"
  ],
  "indexPath": "~/.memory-search/index.db",
  "embeddingEndpoint": "https://your-worker.workers.dev/embedding",
  "embeddingDimensions": 768,
  "chunkMaxTokens": 1000,
  "chunkOverlapTokens": 150,
  "searchTopK": 15,
  "searchCandidateCap": 300,
  "expandQueries": false,
  "provider": "api",
  "localLlm": {
    "embedModel": "embeddinggemma-300M-Q8_0",
    "rerankModel": "qwen3-reranker-0.6b-q8_0",
    "generateModel": "qmd-query-expansion-1.7B-q4_k_m",
    "modelCacheDir": "~/.cache/memory-search/models",
    "inactivityTimeoutMs": 300000
  },
  "collections": [
    {
      "name": "docs",
      "paths": ["/path/to/your/docs"]
    }
  ],
  "pathContexts": [
    {
      "path": "/path/to/your/code",
      "description": "Source code projects"
    }
  ]
}
